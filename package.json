{
  "name": "react-email-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --concurrency 25",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "release": "pnpm build && pnpm changeset publish",
    "canary:version": "(changeset pre enter canary 2>/dev/null || true) && changeset version && pnpm install",
    "version": "(changeset pre exit 2>/dev/null || true) && changeset version && pnpm install",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "@changesets/cli": "2.27.1",
    "@types/node": "18.18.0",
    "@types/react": "18.2.47",
    "@types/react-dom": "18.2.14",
    "eslint": "8.50.0",
    "happy-dom": "12.2.2",
    "prettier": "3.0.3",
    "tsconfig": "workspace:*",
    "tsup": "7.2.0",
    "turbo": "1.13.3",
    "vite": "5.0.13",
    "vitest": "0.34.6"
  },
  "packageManager": "pnpm@9.1.2",
  "pnpm": {
    "patchedDependencies": {
      "postcss-css-variables@0.19.0": "patches/postcss-css-variables@0.19.0.patch",
      "process@0.11.10": "patches/process@0.11.10.patch",
      "tailwindcss@3.3.2": "patches/tailwindcss@3.3.2.patch"
    }
  }
}
