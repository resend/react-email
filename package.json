{
  "name": "react-email-monorepo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build --filter=!demo",
    "dev": "turbo run dev --parallel --concurrency 25",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  },
  "devDependencies": {
    "@types/node": "18.18.0",
    "@types/react": "18.2.33",
    "@types/react-dom": "18.2.14",
    "eslint": "8.50.0",
    "happy-dom": "12.2.2",
    "prettier": "3.0.3",
    "tsconfig": "workspace:*",
    "tsup": "7.2.0",
    "turbo": "1.11.3",
    "vite": "4.5.2",
    "vitest": "0.34.6"
  },
  "packageManager": "pnpm@8.14.1",
  "pnpm": {
    "patchedDependencies": {
      "postcss-css-variables@0.19.0": "patches/postcss-css-variables@0.19.0.patch",
      "tailwindcss@3.3.2": "patches/tailwindcss@3.3.2.patch"
    }
  }
}
