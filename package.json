{
  "name": "repo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "./shared/bootstrap.sh"
  },
  "devDependencies": {
    "@dot/versioner": "^0.1.5",
    "@moonrepo/cli": "^1.13.5",
    "@swc/core": "^1.3.82",
    "@swc/helpers": "^0.5.1",
    "@types/node": "20.6.2",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "csstype": "3.1.2",
    "eslint": "8.49.0",
    "eslint-config-shellscape": "^6.0.1",
    "husky": "^8.0.3",
    "lint-staged": "14.0.1",
    "ts-node": "10.9.1",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --cache"
    ],
    "**/(package|tsconfig(.*)?).json": [
      "prettier --write"
    ],
    "(pnpm-workspace|.github/**/*).{yml,yaml}": [
      "prettier --write"
    ],
    "*package.json": [
      "prettier --write --plugin=prettier-plugin-package"
    ],
    "((.github/**/*)|(README|CHANGELOG)|(**/(README|CHANGELOG))).md": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@8.7.1",
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.22",
      "@types/react-dom": "18.2.7",
      "react": "18.2.0",
      "react-dom": "18.2.0",
      "react-router-dom": "6.16.0"
    }
  }
}